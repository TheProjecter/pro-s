<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="system" namespace="/pages/system" extends="struts-default">
		<action name="user_*" class="com.wozeze.pros.action.system.UserAction" method="{1}">
			<result name="registerPage">/pages/system/user_register.jsp</result>
			<result name="input">/pages/system/user_{1}.jsp</result>
			<result name="login_failure">/pages/system/user_login.jsp</result>
			<result name="register_success">/pages/system/user_login.jsp</result>
			<result name="login_success">/index.jsp</result>
		</action>
	</package>
</struts>