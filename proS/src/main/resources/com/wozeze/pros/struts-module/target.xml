"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="target" namespace="/pages/target" extends="authority">
		<action name="targetAction_*" class="com.wozeze.pros.action.target.TargetAction" method="{1}">
			<result name="updateTargetPage">/pages/target/updateTarget.jsp</result>
			<result name="input">/pages/target/updateTarget.jsp</result>
			<result name="operateSuccess">/pages/common/common_success.jsp</result>
			<result name="operateFailure">/pages/common/common_failure.jsp</result>
			<result name="queryTargets">/pages/target/queryTargets.jsp</result>
		</action>
	</package>
</struts>